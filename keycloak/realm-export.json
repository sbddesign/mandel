      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "cashu-secret",
      "redirectUris": [
        "http://localhost:3000/*",
        "https://mandel-mint.onrender.com/*"
      ],
      "webOrigins": [
